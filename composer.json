{
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-pdo": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "nelmio/cors-bundle": "^1.5",
        "ramsey/uuid": "^3.8",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "archive": {
        "exclude": [
            "docker",
            "tests",
            "var",
            "docker-compose.yml",
            "phpunit.xml.dist",
            "Makefile",
            "README.md",
            ".env",
            ".env.test",
            ".git",
            ".gitignore"
        ]
    }
}
